{
  "multipart": [
    { "when": { "up": true    }, "apply": { "model": "joshsruins:block/mossy_vine", "x": 270, "uvlock": true } },
    { "when": { "north": true }, "apply": { "model": "joshsruins:block/mossy_vine",           "uvlock": true } },
    { "when": { "east": true  }, "apply": { "model": "joshsruins:block/mossy_vine", "y":  90, "uvlock": true } },
    { "when": { "south": true }, "apply": { "model": "joshsruins:block/mossy_vine", "y": 180, "uvlock": true } },
    { "when": { "west": true  }, "apply": { "model": "joshsruins:block/mossy_vine", "y": 270, "uvlock": true } }
  ]
}
